/* Create a program that will generate the multiplication table (up to x10) of a number.  
See the sample output below: 

Enter a number: 6 
Multiplication table of 6: 
1 * 6 = 6 
2 * 6 = 12 
3 * 6 = 18 
4 * 6 = 24 
5 * 6 = 30 
6 * 6 = 36 
7 * 6 = 42 
8 * 6 = 48 
9 * 6 = 54 
10 * 6 = 60 */

// BY LEI-ANN EDANG (ENG 1-12)

#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
	
	int x, y, N;
	
	cout << "Enter a number: ";
	cin >> N;
	cout << "Multiplication table of " << N << ":" << endl;
		
	{
		
		for (y=1; y<=10; y++)
		cout << y << " * " << N << " = " << N*y << endl;
				
	}
	
	_getch();
	return 0; 
	
}
